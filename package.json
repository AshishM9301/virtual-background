{
  "name": "virtual-background",
  "version": "0.1.0",
  "description": "Demo on adding virtual background to a live video stream in the browser",
  "homepage": "https://volcomix.github.io/virtual-background",
  "repository": "https://github.com/Volcomix/virtual-background.git",
  "author": "Volcomix <Volcomix@users.noreply.github.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@tensorflow-models/body-pix": "^2.0.5",
    "@tensorflow/tfjs": "^2.8.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "gh-pages": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "init:tflite": "yarn init:tflite:image && yarn init:tflite:container && yarn init:mediapipe:copy && yarn init:mediapipe:sed",
    "init:tflite:image": "docker build -t tflite tflite",
    "init:tflite:container": "docker run -dit -v $PWD/tflite:/tensorflow_src/tensorflow/lite/virtual_background -v $PWD/wasm:/tflite_wasm -v $PWD/public/tflite:/tflite_build -v $PWD/public/models:/tflite_models --name tflite tflite bash",
    "init:mediapipe:copy": "docker exec tflite bash -c 'cp /mediapipe_src/mediapipe/util/tflite/operations/transpose_conv_bias.* /tensorflow_src/tensorflow/lite/virtual_background'",
    "init:mediapipe:sed": "docker exec tflite bash -c \"sed -i 's/mediapipe\\/util\\/tflite\\/operations\\///' /tensorflow_src/tensorflow/lite/virtual_background/transpose_conv_bias.cc\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:tflite": "docker exec -w /tensorflow_src tflite bazel build --define tflite_with_xnnpack=true -c opt //tensorflow/lite/virtual_background:tflite",
    "build:wasm": "docker exec tflite emcc /tflite_wasm/tflite.cc -o /tflite_build/tflite.wasm -Os --no-entry",
    "test": "react-scripts test",
    "test:tflite": "docker exec tflite /tensorflow_src/bazel-bin/tensorflow/lite/virtual_background/tflite /tflite_models/segm_lite_v681.tflite",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
